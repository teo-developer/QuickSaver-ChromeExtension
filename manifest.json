{
  "manifest_version": 3,
  "name": "QuickSaver",
  "version": "1.0",
  "description": "Save and track your visited websites with insights",
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "alarms"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/logo.png",
      "48": "images/logo.png",
      "128": "images/logo.png"
    }
  },
  "icons": {
    "16": "images/logo.png",
    "48": "images/logo.png",
    "128": "images/logo.png"
  },
  "background": {
    "service_worker": "background.js"
  }
} 